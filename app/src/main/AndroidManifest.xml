<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.truiton.bottomnavigation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.infokos.activity.SearchResultKosActivity"
            android:label="@string/title_activity_search_kos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.infokos.activity.DetailKosActivity"
            android:label="@string/title_activity_detail_kos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>